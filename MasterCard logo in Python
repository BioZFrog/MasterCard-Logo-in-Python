from turtle import *
Red = (234, 0, 27)
Orange = (247, 158, 28, 0.843)
t = Turtle()
s = Screen()
t.width(2.5)
s.bgcolor("White")
t.color("Transparent")
t.backward(68)
t.left(90)
t.backward(90)
t.right(90)
t.color(Red)
t.begin_fill()
t.speed(20)
t.circle(79)
t.end_fill()
t.circle(79,88)
t.right(88)
t.color("transparent")
t.backward(30)
t.right(90)
t.color(Orange)
t.begin_fill()
t.circle(79)
t.end_fill()
